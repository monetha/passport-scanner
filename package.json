{
    "name": "passport-scanner",
    "version": "1.0.0",
    "description": "An app for exploring passports on blockchain",
    "repository": {
        "type": "git",
        "url": "git@github.com/monetha/passport-scanner.git"
    },
    "engines": {
        "npm": ">=3",
        "node": ">=8"
    },
    "author": "Monetha",
    "scripts": {
        "postinstall": "cross-env NODE_ENV=development npm run build:dll",
        "prebuild": "npm run build:clean",
        "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js -p --hide-modules --display-optimization-bailout",
        "build:clean": "rimraf ./build",
        "build:dll": "node ./internals/scripts/dependencies.js",
        "dist:prepare-archives": "node ./internals/scripts/prepare-archives.js",
        "dist:clean": "rimraf ./dist",
        "start": "cross-env NODE_ENV=development node server",
        "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
        "start:production": "npm run build && npm run start:prod",
        "start:prod": "cross-env NODE_ENV=production node server",
        "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*",
        "test": "cross-env NODE_ENV=test jest --coverage",
        "test:watch": "cross-env NODE_ENV=test jest --watchAll"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run dist:clean && npm run dist:build && git add dist"
        }
    },
    "dllPlugin": {
        "path": "node_modules/react-boilerplate-dlls",
        "exclude": [
            "chalk",
            "compression",
            "cross-env",
            "express",
            "ip",
            "minimist",
            "sanitize.css"
        ],
        "include": [
            "core-js",
            "lodash",
            "eventsource-polyfill"
        ]
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.18",
        "@fortawesome/free-solid-svg-icons": "^5.8.2",
        "@fortawesome/react-fontawesome": "^0.1.4",
        "babel-polyfill": "6.26.0",
        "bignumber.js": "^8.1.1",
        "classnames": "^2.2.6",
        "connected-react-router": "^6.4.0",
        "fontfaceobserver": "2.1.0",
        "formik": "^1.5.4",
        "history": "4.9.0",
        "i18n-js": "^3.2.2",
        "immutable-assign": "^2.1.1",
        "lodash": "^4.17.14",
        "minimist": "1.2.0",
        "moment": "^2.24.0",
        "query-string": "^6.5.0",
        "react": "16.8.6",
        "react-dom": "16.8.6",
        "react-redux": "7.0.3",
        "react-responsive": "^6.1.2",
        "react-router-dom": "5.0.0",
        "react-smooth-collapse": "^2.0.1",
        "react-super-responsive-table": "^4.3.12",
        "react-tippy": "^1.2.3",
        "react-transition-group": "^4.0.0",
        "recompose": "^0.30.0",
        "reduce-reducers": "^1.0.4",
        "redux": "4.0.1",
        "redux-saga": "1.0.2",
        "verifiable-data": "2.6.3",
        "web3": "1.0.0-beta.55",
        "whatwg-fetch": "3.0.0",
        "yup": "^0.27.0"
    },
    "devDependencies": {
        "@babel/core": "^7.4.4",
        "@types/classnames": "2.2.7",
        "@types/jest": "^24.0.12",
        "@types/lodash": "^4.14.125",
        "@types/node": "^11.11.3",
        "@types/react": "16.8.8",
        "@types/react-modal": "^3.8.2",
        "@types/react-redux": "7.0.1",
        "@types/react-router": "^5.0.0",
        "@types/react-router-dom": "4.3.3",
        "@types/yup": "^0.26.13",
        "add-asset-html-webpack-plugin": "3.1.3",
        "awesome-typescript-loader": "^5.2.1",
        "babel-cli": "6.26.0",
        "babel-core": "6.26.3",
        "babel-loader": "7.1.5",
        "babel-plugin-dynamic-import-node": "2.0.0",
        "babel-plugin-react-transform": "3.0.0",
        "babel-plugin-syntax-dynamic-import": "6.18.0",
        "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
        "babel-plugin-transform-react-constant-elements": "6.23.0",
        "babel-plugin-transform-react-inline-elements": "6.22.0",
        "babel-preset-env": "1.7.0",
        "babel-preset-react": "6.24.1",
        "babel-preset-stage-0": "6.24.1",
        "circular-dependency-plugin": "5.0.2",
        "compression": "1.7.4",
        "copy-webpack-plugin": "^5.0.3",
        "cross-env": "5.2.0",
        "css-loader": "2.1.1",
        "enzyme": "3.9.0",
        "eventsource-polyfill": "0.9.6",
        "exports-loader": "0.7.0",
        "express": "4.16.4",
        "file-loader": "3.0.1",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "image-webpack-loader": "4.6.0",
        "imports-loader": "0.8.0",
        "ip": "^1.1.5",
        "jest": "^24.8.0",
        "ngrok": "3.1.1",
        "node-plop": "0.18.0",
        "node-sass": "4.12.0",
        "null-loader": "1.0.0",
        "plop": "2.3.0",
        "react-svg-loader": "^3.0.3",
        "react-test-renderer": "16.8.6",
        "redux-devtools-extension": "^2.13.8",
        "rimraf": "2.6.3",
        "sass-loader": "7.1.0",
        "shelljs": "0.8.3",
        "string-replace-loader": "2.1.1",
        "style-loader": "0.23.1",
        "ts-jest": "^24.0.2",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "tslint": "^5.16.0",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-lines-between-class-members": "^1.3.1",
        "typescript": "^3.4.5",
        "url-loader": "1.1.2",
        "webpack": "4.31.0",
        "webpack-cli": "^3.3.2",
        "webpack-dev-middleware": "3.6.2",
        "webpack-hot-middleware": "2.24.4"
    }
}
